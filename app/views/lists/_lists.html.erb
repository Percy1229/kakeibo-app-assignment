<table border=1>
    <thead>
        <tr>
            <th>Date</th>
            <th>Item</th>
            <th>Expense</th>
            <th>Place</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <% lists.each do |list| %>
            <td>
                <%= list.date %>
            </td>
             <td>
                <%= list.item_name %>
            </td>
             <td>
                <span>Â¥</span><%= list.expense %>
            </td>
             <td>
                <%= list.place %>
            </td>
            <td>
                <%= link_to "Edit", edit_list_path(list), class: 'btn btn-primary btn-sm' %>
            </td>
            <td>
                <%= link_to "Delete", list, method: :delete, data: { confirm: "Are you sure?"}, class: 'btn btn-danger btn-sm' %>
            </td>
            <% end %>
        </tr>
    </tbody>
</table>
